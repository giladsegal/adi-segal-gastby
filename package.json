{
  "name": "adi-segal-gatsby",
  "private": true,
  "description": "The website of the photographer Adi Segal",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "WEBSITE_TYPE=weddings gatsby build",
    "build:deploy-preview": "GATSBY_DEPLOY_PREVIEW=true npm run build",
    "develop": "WEBSITE_TYPE=weddings gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "WEBSITE_TYPE=weddings npm run develop",
    "serve": "WEBSITE_TYPE=weddings gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "generate:fonts": "./scripts/generate-fonts.sh && npm run preload:fonts",
    "preload:fonts": "gatsby-preload-fonts"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^14.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.66",
    "gatsby-image": "^2.4.20",
    "gatsby-plugin-manifest": "^2.4.34",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-sass": "^2.3.14",
    "gatsby-plugin-sharp": "^2.6.38",
    "gatsby-source-contentful": "^2.3.49",
    "gatsby-source-filesystem": "^2.3.32",
    "gatsby-transformer-sharp": "^2.5.16",
    "node-sass": "^4.14.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@contentful/rich-text-types": "^14.1.1",
    "@types/classnames": "^2.2.10",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "gatsby-plugin-preload-fonts": "^1.2.28",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sitemap": "^2.4.15",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "normalize.css": "^8.0.1",
    "prettier": "2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giladsegal/adi-segal-gastby"
  },
  "bugs": {
    "url": "https://github.com/giladsegal/adi-segal-gastby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": "prettier --write"
  }
}
